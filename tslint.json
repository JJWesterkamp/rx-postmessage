{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "max-line-length": false,
        "no-namespace": [true, "allow-declarations"],
        "variable-name": false,
        "typedef-whitespace": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
